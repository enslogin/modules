    if (!value) {
      return;
    }

    this._bridge = value;
  }

  get bridge() {
    return this._bridge;
  }

  set key(value) {
    if (!value) {
      return;
    }

    const key = utils_1.convertHexToArrayBuffer(value);
    this._key = key;
  }

  get key() {
    if (this._key) {
      const key = utils_1.convertArrayBufferToHex(this._key, true);
      return key;
    }

    return "";
  }

