      const res = yield this.handleRequest(payload);

      if (res.result) {
        return res.result;
      } else {
        if (res.error && res.error.message) {
          throw new Error(res.error.message);
        } else {
          throw new Error("Failed JSON-RPC request");
        }
      }
    });
  }

